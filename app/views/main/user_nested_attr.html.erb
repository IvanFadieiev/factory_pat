<%= form_for @user, url: create_path do |f| %>
    <%= hidden_field_tag :sent_to, 'create_user' %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>
    <%= f.fields_for :address do |ff| %>
      <%= ff.label :street %>
      <%= ff.text_field :street %>
    <% end %>
    <br>
    <%= f.submit 'Save' %>
<% end %>